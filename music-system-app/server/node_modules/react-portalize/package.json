    "compile": "babel src -x .ts,.tsx --ignore \"**/*.d.ts\",\"**/*.test.tsx\" --delete-dir-on-start",
    "compile-server": "babel server/src -x .ts,.tsx --ignore \"**/*.d.ts\",\"**/*.test.tsx\" --delete-dir-on-start",
